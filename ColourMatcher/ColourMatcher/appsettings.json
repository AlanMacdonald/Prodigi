{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ImageOptions": {
    "ColorMatchPixelX": 0,
    "ColorMatchPixelY": 0,
    //The tolerance for a colours RGB values to be close to another colour in the catalogue.
    //e.g. 2 means a color with all rgb values within 2 of a catalogue colour will be a match. (152, 158, 151) would match (150, 150, 150)
    "RgbFuzziness": 2
  }
}
